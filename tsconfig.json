{
  "include": ["src"],
  "compilerOptions": {
    "noEmit": true,
    "target": "es2019",
    "module": "es2020",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "jsxImportSource": "react",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
